{
    "defaultSeverity": "error",
    "extends": ["tslint:latest", "tslint-react"],
    "jsRules": {},
    "rules": {
        "interface-name": false,
        "semicolon": [true, "never"],
        "member-access": [false],
        "ordered-imports": [true],
        "no-console": [false],
        "no-var-requires": true,
        "quotemark": [true, "double", "jsx-double"]
    },
    "rulesDirectory": []
}